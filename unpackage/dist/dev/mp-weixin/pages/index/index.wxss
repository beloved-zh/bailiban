page {
  background-color: #f2f2f2;
}
swiper {
  height: 340rpx;
}
swiper image {
  width: 100%;
  height: 100%;
}
.nav {
  background-color: #FFFFFF;
  height: 200rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-bottom: 30rpx;
}
.nav .nav_time {
  width: 25%;
  text-align: center;
}
.nav .nav_time .iconfont {
  width: 110rpx;
  height: 110rpx;
  background-color: #12c494;
  border-radius: 50%;
  margin: 30rpx auto 20rpx auto;
  line-height: 110rpx;
  color: #ffF;
  font-size: 60rpx;
}
.nav .nav_time text {
  font-size: 30rpx;
  color: #5f5f5f;
}
.main {
  width: 100%;
}
.main .noLogin {
  margin: 100rpx auto;
  text-align: center;
}
.main .noLogin image {
  width: 300rpx;
  height: 300rpx;
}
.main .noLogin view {
  color: #12c494;
}
.main .exercise {
  width: 700rpx;
  margin: 0 25rpx;
}
.main .exercise .exercise-row {
  width: 700rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.main .exercise .exercise-row .exercise-item {
  width: 340rpx;
  height: 500rpx;
  background-color: #FFFFFF;
  border-radius: 7px;
  text-align: center;
}
.main .exercise .exercise-row .exercise-item .parentName {
  font-size: 38rpx;
  line-height: 100rpx;
}
.main .exercise .exercise-row .exercise-item .name {
  font-size: 32rpx;
  color: #666666;
}
.main .exercise .exercise-row .exercise-item .questionNum {
  font-size: 28rpx;
  color: #a2a2a2;
  line-height: 60rpx;
}
.main .exercise .exercise-row .exercise-item image {
  width: 250rpx;
  height: 250rpx;
}
.main .exercise .exercise-row .exercise-item .not {
  font-size: 32rpx;
  color: #666666;
  line-height: 200rpx;
}
.main .work {
  width: 700rpx;
  margin: 0 25rpx;
}
.main .work .work-time {
  background-color: #FFFFFF;
  width: 100%;
  height: 300rpx;
  border-radius: 7px;
  margin-bottom: 20rpx;
}
.main .work .work-time .work-main {
  width: 640rpx;
  height: 190rpx;
  margin: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.main .work .work-time .work-main .left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.main .work .work-time .work-main .left .work-time-name {
  font-size: 32rpx;
  font-weight: bold;
}
.main .work .work-time .work-main .left .work-time-sum {
  font-size: 28rpx;
}
.main .work .work-time .work-main .left .work-time-sum text:nth-child(1) {
  color: #666666;
}
.main .work .work-time .work-main .left .work-time-sum text:nth-child(2) {
  margin-left: 50rpx;
  color: #12c494;
}
.main .work .work-time .work-main .right {
  margin: auto 0;
}
.main .work .work-time .work-main .right button {
  color: #FFFFFF;
  background-color: #12c494;
  border-radius: 30px;
  width: 200rpx;
  height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
}
.head {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.head view:nth-child(1) {
  width: 350rpx;
  text-align: left;
}
.head view:nth-child(2) {
  width: 350rpx;
  text-align: right;
  color: #666666;
  font-size: 28rpx;
}
.type {
  width: 150rpx;
  height: 50rpx;
  background-color: #ff6e2e;
  border-radius: 15rpx 0;
  color: #FFFFFF;
  font-size: 25rpx;
  line-height: 50rpx;
  text-align: center;
}

