<view class="content"><swiper indicator-dots autoplay circular interval="1000" indicator-color="#ffffff" indicator-active-color="#12c494"><block wx:for="{{swipers}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><swiper-item><image mode="aspectFill" src="{{item.imageDisplay}}"></image></swiper-item></block></swiper><view class="nav"><block wx:for="{{nav}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navItemClick',['$0'],[[['nav','',index,'path']]]]]]]}}" class="nav_time" bindtap="__e"><view class="{{[item.icon]}}"></view><text>{{item.title}}</text></view></block></view><view class="main"><block wx:if="{{flag}}"><navigator class="noLogin" url="../login/login" hover-class="none"><image mode="aspectFit" src="../../static/icon/book.png"></image><view>登录后查看练习记录</view></navigator></block><block wx:else><view><view class="exercise"><view class="head">最近错题</view><view class="exercise-row"><view class="exercise-item"><view class="type">练习题库</view><block wx:if="{{exercise.length>=1}}"><view><view class="parentName">{{exercise[0].parentName}}</view><view class="name">{{exercise[0].name}}</view><view class="questionNum">{{"共"+exercise[0].questionNum+"道题"}}</view></view></block><block wx:else><view class="not">暂无题目</view></block><image mode="aspectFit" src="../../static/icon/zjct01.png"></image></view><view class="exercise-item"><view class="type">面试题库</view><block wx:if="{{exercise.length>=2}}"><view><view class="parentName">{{exercise[1].parentName}}</view><view class="name">{{exercise[1].name}}</view><view class="questionNum">{{"共"+exercise[1].questionNum+"道题"}}</view></view></block><block wx:else><view class="not">暂无题目</view></block><image mode="aspectFit" src="../../static/icon/zjct02.png"></image></view></view><view class="head">最近收藏</view><view class="exercise-row"><view class="exercise-item"><view class="type">练习题库</view><block wx:if="{{exercise.length>=3}}"><view><view class="parentName">{{exercise[2].parentName}}</view><view class="name">{{exercise[2].name}}</view><view class="questionNum">{{"共"+exercise[2].questionNum+"道题"}}</view></view></block><block wx:else><view class="not">暂无题目</view></block><image mode="aspectFit" src="../../static/icon/zjsc01.png"></image></view><view class="exercise-item"><view class="type">面试题库</view><block wx:if="{{exercise.length>=4}}"><view><view class="parentName">{{exercise[3].parentName}}</view><view class="name">{{exercise[3].name}}</view><view class="questionNum">{{"共"+exercise[3].questionNum+"道题"}}</view></view></block><block wx:else><view class="not">暂无题目</view></block><image mode="aspectFit" src="../../static/icon/zjsc02.png"></image></view></view></view><view class="work"><view class="head"><view>我的作业</view><view><navigator url="../homework/homework" hover-class="none">更多></navigator></view></view><block wx:for="{{homeworks}}" wx:for-item="item" wx:for-index="index" wx:key="id"><block wx:if="{{index<2}}"><view class="work-time"><view class="type">上机作业</view><view class="work-main"><view class="left"><view class="work-time-name">{{item.firstTagName+"-"+item.secondTagName}}</view><view class="work-time-sum"><text>{{"任务:"+item.workSum}}</text><text>{{"提交:"+item.submitSum}}</text></view></view><view class="right"><button data-event-opts="{{[['tap',[['goHomeworkItem',['$0','$1','$2','$3'],[[['homeworks','id',item.id,'classId']],[['homeworks','id',item.id,'createBy']],[['homeworks','id',item.id,'firstTagId']],[['homeworks','id',item.id,'secondTagId']]]]]]]}}" bindtap="__e">去查看</button></view></view></view></block></block></view></view></block></view></view>