page {
  background-color: #f2f2f2;
}
.work {
  width: 700rpx;
  margin: 25rpx;
}
.work .work-time {
  background-color: #FFFFFF;
  width: 100%;
  height: 200rpx;
  border-radius: 7px;
  margin-top: 25rpx;
}
.work .work-time .work-main {
  width: 640rpx;
  height: 160rpx;
  /* margin: auto; */
  padding: 20rpx 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: space-between;
  -webkit-align-items: space-between;
          align-items: space-between;
}
.work .work-time .work-main .left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.work .work-time .work-main .left .work-time-name {
  font-size: 32rpx;
  font-weight: bold;
}
.work .work-time .work-main .left .work-time-sum {
  font-size: 28rpx;
}
.work .work-time .work-main .left .work-time-sum text:nth-child(1) {
  color: #666666;
}
.work .work-time .work-main .left .work-time-sum text:nth-child(2) {
  margin-left: 50rpx;
  color: #12c494;
}
.work .work-time .work-main .right {
  margin: auto 0;
}
.work .work-time .work-main .right button {
  color: #FFFFFF;
  background-color: #12c494;
  border-radius: 30px;
  width: 200rpx;
  height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
}
.not {
  width: 100%;
  height: 750rpx;
  text-align: center;
}
.not image {
  width: 450rpx;
  height: 300rpx;
  margin: 300rpx 150rpx 50rpx 150rpx;
}
.not view {
  font-size: 20rpx;
}

