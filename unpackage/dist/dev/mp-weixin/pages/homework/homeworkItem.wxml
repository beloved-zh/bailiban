<view><block wx:for="{{homeworkItem}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="homework-item"><view class="head"><view class="name">{{item.name}}</view><view class="score"><block wx:if="{{item.homeworkLogInfo!=null}}"><text><block wx:for="{{item.homeworkLogInfo.score}}" wx:for-item="n" wx:for-index="__i1__"><text class="iconfont icon-xingxing1"></text></block><block wx:for="{{5-item.homeworkLogInfo.score}}" wx:for-item="n" wx:for-index="__i2__"><text class="iconfont icon-xingxing"></text></block><block wx:for="{{5}}" wx:for-item="n" wx:for-index="__i3__"><block wx:if="{{item.homeworkLogInfo.score==null}}"><text class="iconfont icon-xingxing"></text></block></block></text></block><block wx:else><block wx:for="{{5}}" wx:for-item="n" wx:for-index="__i4__"><text class="iconfont icon-xingxing"></text></block></block></view></view><view class="main"><view class="left"><view class="thirdTagName">{{item.thirdTagName}}</view><view class="expireDate"><text class="iconfont icon-shizhong"></text>{{item.expireDate}}</view></view><block wx:if="{{item.homeworkLogInfo!=null}}"><view class="right" style="color:#24b38d;">已提交</view></block><block wx:if="{{item.homeworkLogInfo==null}}"><view class="right" style="color:#666666;">未提交</view></block></view></view></block></view>